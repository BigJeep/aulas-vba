Private Sub btn_entrar_Click()
    usuario = "adm"
    email = "fatec@fatec"
    senha = "adm"
    
    If (txt_usuario = usuario Or txt_usuario = email) And txt_senha = senha Then
        MsgBox ("Conta logada com sucesso!"), vbInformation + vbOKOnly, "AVISO"
        
        Application.Visible = True
        'frm_login.Hide
        Unload Me 'descarrega o form
        Planilha1.Select
        Application.StatusBar = "Usuario Logado"
    Else
        MsgBox ("Conta invalida"), vbExclamation + vbOKOnly, "AVISO"
        txt_usuario = Empty
        txt_senha = Empty
        txt_usuario.SetFocus 'foca no elemento
    End If
    

End Sub

